{
  "version": 2,
  "framework": "nextjs",
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/status",
      "dest": "/api/status.js"
    },
    {
      "src": "/api/inspections(.*)",
      "dest": "/api/visiocar.js"
    },
    {
      "src": "/api/deposits(.*)",
      "dest": "/api/visiocar.js"
    },
    {
      "src": "/api/expenses(.*)",
      "dest": "/api/visiocar.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/attached_assets/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "echo \"Skipping build\"",
  "installCommand": "echo \"Skipping install\"",
  "outputDirectory": "attached_assets",
  "public": true
}