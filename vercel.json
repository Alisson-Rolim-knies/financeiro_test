{
  "version": 2,
  "routes": [
    {
      "src": "/api/status",
      "dest": "/api/status.js"
    },
    {
      "src": "/api/inspections(.*)",
      "dest": "/api/visiocar.js"
    },
    {
      "src": "/api/deposits(.*)",
      "dest": "/api/visiocar.js"
    },
    {
      "src": "/api/expenses(.*)",
      "dest": "/api/visiocar.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/attached_assets/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}