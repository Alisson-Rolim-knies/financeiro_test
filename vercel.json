{
  "version": 2,
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "pages/api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/visiocar.html", "dest": "/api/visiocar" },
    { "src": "/app", "dest": "/api/visiocar" },
    { "src": "/app/status", "dest": "/api/status" },
    { "handle": "filesystem" }
  ]
}